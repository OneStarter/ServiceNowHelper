{
    "version": "1.2.2",
    "name": "ServiceNow Helper",
    "short_name": "SN Helper",
    "author": "OneStarter",
    "description": "ServiceNow Helper allows you to use ServiceNow faster and in a more efficient way.",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon.png"
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}